import java.util.Scanner;
public class nesteadNo1 {
    public static void main(String[] args) {
        Scanner scn = new Scanner (System.in);
        int j; //membuat memori j tipe data integer
        System.out.print("Masukkan baris dan kolom (N x N): ");
        int n = scn.nextInt(); //baca memori n tipe data integer
        
        System.out.println("Kolom "+n+ " dan baris "+n+ " adalah: ");

        //melakukan looping dengan isi memori i
        for (int i = 0; i < n; i++) {

            //melakukan looping dengan isi memori j
            for (j = 0; j < n; j++) {

                //percabngan untuk menentukan sesuai kondisi
                if (i == j) {
                    System.out.print("1");
                }else {
                    System.out.print("0");
                }
            }
            System.out.println();
        }
    }
}
